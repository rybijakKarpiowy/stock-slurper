generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Asgard {
  created_at DateTime @default(now()) @db.Date
  name       String    @db.VarChar
  code       String    @db.VarChar
  price      Float     @db.Real
  amount     BigInt
  link       String    @db.VarChar
  id         BigInt    @id @default(autoincrement())
}

model Axpol {
  created_at DateTime @default(now()) @db.Date
  name       String    @db.VarChar
  code       String    @db.VarChar
  price      Float     @db.Real
  amount     BigInt
  link       String    @db.VarChar
  id         BigInt    @id @default(autoincrement())
}

model Par {
  created_at DateTime @default(now()) @db.Date
  name       String    @db.VarChar
  code       String    @db.VarChar
  price      Float     @db.Real
  amount     BigInt
  link       String    @db.VarChar
  id         BigInt    @id @default(autoincrement())
}
